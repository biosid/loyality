@model Vtb24.Site.Models.Catalog.OnlineCategoryModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = Model.Name;
    ViewBag.MainMenu = "Catalog";
}
@Html.AddScriptBlock(
    @<script>
         require(['Shared/frame-resizer'], function(resizer) {
             resizer({ iframe: $('iframe#online-cat') });
         });
     </script>
)
@Html.Partial("_ProcessCommand", "iframe#online-cat")
<div class="full-content" style="min-height:200px">
    <iframe id="online-cat" src="@Model.CategoryUrl" width="100%" height="450" scrolling="no" marginheight="0" marginwidth="0" frameborder="0" allowfullscreen></iframe>
</div>
