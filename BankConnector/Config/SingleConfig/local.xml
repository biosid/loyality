      <ReplaceTo>0 0 0 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки задачи получения ответа по списку промоакций -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@PromoActionReceiveChron@</string>
      </Patterns>
      <!-- Каждый день в 12:00:00, то есть через 12 часов после "Отправка списка промоакций" -->
      <ReplaceTo>0 0 12 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки задачи загрузка сообщений для ЛК клиентов -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@PersonalMessagesChron@</string>
      </Patterns>
      <!--Периодичность выгрузки исходного файла: ежедневно (точное время будет оговорено позднее).-->
      <ReplaceTo>0 0 12 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки отключения участников -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@SendDetachListChron@</string>
      </Patterns>
      <!--Периодичность выгрузки исходного файла: ежедневно, в 19:00-->
      <ReplaceTo>0 0 19 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@ReceiveDetachCountChron@</string>
      </Patterns>
      <!--Периодичность выгрузки ответного файла: не позднее 12 часов после обработки файла-->
      <ReplaceTo>0 0 7 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки задачи Регистрации клиентов банка -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@RegisterBankClientsChron@</string>
      </Patterns>
      <!-- Периодичность выгрузки исходного файла: ежедневно, в 09:00. Периодичность выгрузки ответного файла: не позднее 12 часов после выгрузки файла из Системы лояльности -->
      <ReplaceTo>0 0 15 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки задачи Загрузки продуктов банка -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@RegisterBankOffersChron@</string>
      </Patterns>
      <!-- Периодичность выгрузки исходного файла: ежедневно, в 09:00. Периодичность выгрузки ответного файла: не позднее 12 часов после выгрузки файла из Системы лояльности -->
      <ReplaceTo>0 0 9 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Привязка клиентов к целевым аудиториям -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@AssignClientTargetAudienceChron@</string>
      </Patterns>
      <!-- Периодичность выгрузки исходного файла: ежедневно (точное время будет оговорено позднее/ Периодичность выгрузки ответного файла: не позднее 12 часов после обработки файла -->
      <ReplaceTo>0 0 12 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки начисления баллов -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@ReceiveAccrualsChron@</string>
      </Patterns>
      <!--Периодичность выгрузки исходного файла: ежедневно (точное время будет оговорено позднее). -->
      <ReplaceTo>0 0 12 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@SendAccrualsChron@</string>
      </Patterns>
      <!--Периодичность выгрузки ответного файла: не позднее 12 часов после выгрузки файла из Банка -->
      <ReplaceTo>0 0 0 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки рассылки оппвещений WishList -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@WishListNotificationsChron@</string>
      </Patterns>
      <!--Каждую полночь -->
      <ReplaceTo>0 0 0 * * ?</ReplaceTo>
    </ConfigFileTransformation>

