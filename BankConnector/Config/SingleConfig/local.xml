    <!-- Настройки активации участников -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <!--Периодичность выгрузки исходного файла: ежедневно, в 24:00/ Периодичность выгрузки ответного файла: не позднее 12 часов после обработки файла-->
        <string>@ActivateClientsChron@</string>
      </Patterns>
      <ReplaceTo>0 0 22 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки отправки запросов на регистрацию -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@SendRegistrationClientsChron@</string>
      </Patterns>
      <!--Периодичность выгрузки исходного файла: два раза в день, в 10:00 для зарегистрировавшихся с 19:00 прошлого дня до 10:00 текущего дня; в 19:00 для зарегистрировавшихся с 10:00 до 19:00 текущего дня-->
      <ReplaceTo>0 0 10,19 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки получения результата регистрации от банка -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@ReceiveRegistrationClientsChron@</string>
      </Patterns>
      <!--Периодичность выгрузки ответного файла: не позднее 12 часов после выгрузки файла из Системы лояльности-->
      <ReplaceTo>0 0 7,22 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки отправки и получения заказов -->
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@SendOrdersChron@</string>
      </Patterns>
      <!--Периодичность выгрузки исходного файла: ежедневно в 11:00 -->
      <ReplaceTo>0 0 11 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <ConfigFileTransformation xsi:type="StringReplaceConfigFileTransformation">
      <Patterns>
        <string>@ReceiveOrdersChron@</string>
      </Patterns>
      <!--Не позднее 12 часов после выгрузки-->
      <ReplaceTo>0 0 23 * * ?</ReplaceTo>
    </ConfigFileTransformation>
    <!-- Настройки задачи отправки списка промоакций -->
